function WriteBaseAddress_DWORD(Lib, Offset, Edit)
    local Ranges = gg.getRangesList(Lib)
    if Ranges and #Ranges > 0 then
        local v = {}
        v[1] = {}
        v[1].address = Ranges[1].start + Offset
        v[1].value = Edit
        v[1].flags = 4
        v[1].freeze = true
gg.setValues(v)
    end
end

function isPowerOf(num, base)
  if num == 1 then
    return true
  elseif num < 1 or num % base > 0 then
    return false
  else
    return isPowerOf(num / base, base)
  end
end
function invLog(num, base)
  if num == 0 then
    return 0
  elseif num == 1 then
    return 1
  elseif base == 1 then
    return math.exp(num)
  else
    local log_bc = math.log(base)
    if isPowerOf(num, base) then
      return math.floor(math.log(num) / log_bc)
    else
      return math.floor((math.log(num) / log_bc) + 1)
    end
  end
end
function antilog(num, base)
  return math.pow(base, num)
end
local num = 1000
local base = 10

function setvalue(address,flags,value) local tt={} tt[1]={} tt[1].address=address tt[1].flags=flags tt[1].value=value gg.setValues(tt) end


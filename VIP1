function anti()
gg.searchNumber("2621443;1;52;6;288;4;308;188;29616;16;37352;256;32514;5;4096;39648;35552;36360;40456", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()
gg.getResults(99999)
gg.editAll("0", gg.TYPE_DWORD)
gg.clearResults()
gg.toast("·¥Ä…¥·¥õ…™  Ä·¥á·¥ò·¥è Ä·¥õ ·¥Ä·¥Ñ·¥õ…™·¥†·¥Ä·¥õ·¥á·¥Ö")
end












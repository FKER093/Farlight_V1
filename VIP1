HOME = 1
function HOME()
MN = gg.choice({
"          \n                            ùêÅùêòùêèùêÄùêíùêí ùêåùêÑùêåùêéùêëùêò\n          ",
"          \n                            ùêåùêÑùêåùêéùêëùêò ùêáùêÄùêÇùêä\n          ",
"          \n                                     ùêÑùêóùêàùêì\n          "
}, nil, (os.date("                                   ‚ô°·¥è·¥°…¥ : Íú∞·¥ã·¥á Ä‚ô° \n               ‚Ä¢ %A, %d %B %Y %H:%M%p ‚Ä¢ ")))
if MN == nil then
else
if MN == 1 then BP9() end
if MN == 2 then BP3() end
if MN == 3 then Exit() end end
PUBGMH = -1
end


